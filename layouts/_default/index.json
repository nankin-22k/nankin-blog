[
  {{- /* 「secret_logs」セクション以外の記事だけを集める */ -}}
  {{- $pages := where site.RegularPages "Section" "!=" "secret_logs" -}}
  {{- range $i, $e := $pages }}
  {{- if $i }}, {{ end }}
  {
    "title": {{ .Title | jsonify }},
    "content": {{ .Plain | jsonify }},
    "permalink": {{ .Permalink | jsonify }},
    "summary": {{ .Summary | jsonify }}
  }
  {{- end }}
]